{
    "name": "sankaest/yii2-notification",
    "description": "This module for renders a message from session flash (with ajax, pjax support and etc.)",
    "version": "1.0.0",
    "type": "yii2-extension",
    "keywords": [
        "yii2", "module", "notification", "flash",
        "noty", "toast", "toastr", "growl", "alert", "notyjs", "notyfy", "pnotyfy", "notific8", "lobibox", "sweetalert"
    ],
    "homepage": "https://github.com/sankaest/yii2-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksander Andre",
            "email": "aleksander@axendo.ee",
            "homepage": "https://axendo.ee",
            "role": "Developer"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2"
    },
    "autoload": {
        "psr-4": {
            "sankaest\\modules\\notification\\": "src/"
        }
    },
    "extra": {
        "bootstrap": "sankaest\\modules\\notification\\Bootstrap"
    }
}
